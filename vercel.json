{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/acceder",
            "destination": "/index.html"
        },
        {
            "source": "/crear-cuenta",
            "destination": "/index.html"
        },
        {
            "source": "/crear-cuenta/contratista",
            "destination": "/index.html"
        },
        {
            "source": "/crear-cuenta/contratista/crear-contrase√±a",
            "destination": "/index.html"
        },
        {
            "source": "/crear-cuenta/contratista/hoja-de-vida",
            "destination": "/index.html"
        },
        {
            "source": "/inicio",
            "destination": "/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}